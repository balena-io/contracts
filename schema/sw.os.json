{
  "$id": "sw.os.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "properties": {
    "slug": {
      "$id": "/properties/slug",
      "type": "string",
      "title": "The Slug Schema ",
      "default": "",
      "examples": [
        "debian"
      ]
    },
    "type": {
      "$id": "/properties/type",
      "type": "string",
      "title": "The Type Schema ",
      "default": "",
      "examples": [
        "sw.os"
      ]
    },
    "version": {
      "$id": "/properties/version",
      "type": "string",
      "title": "The Version Schema ",
      "default": "",
      "examples": [
        "1"
      ]
    },
    "data": {
      "$id": "/properties/data",
      "type": "object",
      "properties": {
        "libc": {
          "$id": "/properties/data/properties/libc",
          "type": "string",
          "title": "The Libc Schema ",
          "default": "",
          "examples": [
            "glibc"
          ]
        }
      }
    },
    "requires": {
      "$id": "/properties/requires",
      "type": "array",
      "items": {
        "$id": "/properties/requires/items",
        "type": "object",
        "properties": {
          "type": {
            "$id": "/properties/requires/items/properties/type",
            "type": "string",
            "title": "The Type Schema ",
            "default": "",
            "examples": [
              "sw.blob"
            ]
          },
          "slug": {
            "$id": "/properties/requires/items/properties/slug",
            "type": "string",
            "title": "The Slug Schema ",
            "default": "",
            "examples": [
              "tini"
            ]
          }
        }
      }
    },
    "variants": {
      "$id": "/properties/variants",
      "type": "array",
      "items": {
        "$id": "/properties/variants/items",
        "type": "object",
        "properties": {
          "version": {
            "$id": "/properties/variants/items/properties/version",
            "type": "string",
            "title": "The Version Schema ",
            "default": "",
            "examples": [
              "wheezy"
            ]
          },
          "name": {
            "$id": "/properties/variants/items/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
              "Debian Wheezy"
            ]
          },
          "requires": {
            "$id": "/properties/variants/items/properties/requires",
            "type": "array",
            "items": {
              "$id": "/properties/variants/items/properties/requires/items",
              "type": "object",
              "properties": {
                "or": {
                  "$id": "/properties/variants/items/properties/requires/items/properties/or",
                  "type": "array",
                  "items": {
                    "$id": "/properties/variants/items/properties/requires/items/properties/or/items",
                    "type": "object",
                    "properties": {
                      "type": {
                        "$id": "/properties/variants/items/properties/requires/items/properties/or/items/properties/type",
                        "type": "string",
                        "title": "The Type Schema ",
                        "default": "",
                        "examples": [
                          "arch.sw"
                        ]
                      },
                      "slug": {
                        "$id": "/properties/variants/items/properties/requires/items/properties/or/items/properties/slug",
                        "type": "string",
                        "title": "The Slug Schema ",
                        "default": "",
                        "examples": [
                          "armel"
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
